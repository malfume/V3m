        local HideName = Misc1:CreateButton("Hide Name", function()
            b = client.PlayerGui
            c = b.WorldInfo
            d = c.InfoFrame
            e = d.CharacterInfo
            f = b.LeaderboardGui
            g = f.MainFrame
            h = g.ScrollingFrame
    
            e.Visible = false
    
            for _, name in pairs(h:GetChildren()) do
                if name:IsA'Frame' then
                    if name.Player.Text:match(e.Character.Text) then
                        name.Visible = false
                    end
                end
            end
        end)
    
